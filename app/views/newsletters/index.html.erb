<p>
    <%= link_to 'New', new_war_path %>
    </p>
<h1 class="text-center">JEFA Newsletter</h1>
<div class="section ml-5 mr-5">
  <table class="table table-striped table-responsive">
    <h1 class="h3 text-center"></h1>
    <thead>
      <tr>
        <th>Title</th>
        <th>Year</th>
        <th>Volume</th>
        <th>Version</th>
        <th>Content</th>
        <th>Image</th>
        <th>File</th>
      </tr>
    </thead>
      <tbody>
          <% @newsletters.each do |newsletter| %>
          <tr>
            <td><%= link_to @newsletter.display_name, @newsletter %>
            </td>
            <td><%= newsletter.year %></td>
            <td><%= newsletter.volume %></td>
            <td><%= newsletter.version %></td>
            <td><%= newsletter.content %></td>
            <td><%= newsletter.image %></td>
            <td><%= newsletter.file_name %></td>
            <%#= rails_service_blob_url(newsletter.file_name, disposition: "attachment") %>
          </tr>
          <% end %>
      </tbody>
  </table>
</div>

<%= render "list" %>


